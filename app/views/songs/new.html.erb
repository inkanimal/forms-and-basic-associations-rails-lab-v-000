<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name, :artist %>
  <%= f.text_field :artist_name %>
    <%= f.text_field :genre_name, list: "genres_autocomplete" %>
  <datalist id="genres_autocomplete">
    <% Genre.all.each do |g| %>
      <option value="<%= g.name %>">
    <% end %>
  </datalist>
  <textarea name="song[genre]"></textarea>
  <br />

  <%= f.submit %>
<% end %>


